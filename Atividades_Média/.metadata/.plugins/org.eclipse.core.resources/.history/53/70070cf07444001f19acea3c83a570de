package formaGeometrica;

public class quadrado implements formaGeometrica {

	private double area;
	
	public quadrado(double area) {
		this.area = area;
	}
	
	public double calcularArea() {
		double a = area * 2;
		
		return a;
	}
	
	public double calcularPerimetro() {
		double b = area * 4;;
		
		return b;
	}
}
